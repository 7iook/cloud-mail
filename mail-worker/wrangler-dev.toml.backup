name = "cloud-mail-dev"
main = "src/index.js"
compatibility_date = "2025-06-04"
keep_vars = true

[observability]
enabled = true

[dev]
ip = "0.0.0.0"

[[d1_databases]]
binding = "db"
database_name = "email"
database_id = "a4c1a63a-6ef5-4e6d-8e8c-b6d9e8feb810"

[[kv_namespaces]]
binding = "kv"
id = "2io01d4b299e481b9de060ece9e7785c"

#[[r2_buckets]]
#binding = "r2"
#bucket_name = "email"

[assets]
binding = "assets"
directory = "./dist"
